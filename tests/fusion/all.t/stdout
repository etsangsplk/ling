test2 =
  proc ()
  let x0 : Int = 1.
  let z0 : Int = (x0 + 1).
  let x1 : Int = (z0 + 5).
  let y0 : Int = (x1 + x0 + 2).
  let x2 : Int = (y0 + 4).
  let z1 : Int = (x2 + x1 + x0 + 3)

