my_zero =
  0

assert
  (S : Session)-> (T : Session)-> < S ^ 5, T ^ (0 + my_zero)>
  =
  (S : Session)-> (T : Session)-> < S ^ 2, T ^ 0, S, S, S > : Type

assert
  (S : Session)-> (m : Int)-> (n : Int)-> < S ^ (m + n)>
  =
  (S : Session)-> (m : Int)-> (n : Int)-> < S ^ m, S ^ n > : Type

