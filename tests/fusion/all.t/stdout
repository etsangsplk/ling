  Int2Double 42
  =
  42.0

assert
  showInt 42
  =
  "42"

assert
  showDouble 3.14
  =
  "3.14"

assert
  showChar 'a'
  =
  "'a'"

assert
  showString "Hello \"World\"!"
  =
  "\"Hello \\\"World\\\"!\""

assert
  "Hello " ++S "World!"
  =
  "Hello World!"

replicate =
  \ (A : Type)-> \ (n : Int)-> \ (x : A)-> proc (os : [!A ^ n])
  split os [o ^ n].
  slice (o)n as _
    o <- x

replicate_par =
  proc (c : {!Int ^ 10})
  split c {d ^ 10}.
  slice (d)10 as i
    d <- i

replicate_proc =
  \ (A : Type)-> \ (n : Int)-> proc (c : {?A, [!A ^ n]})
  split c {i, os}.
  split os [o ^ n].
  slice (o)n as _
    let x#i : A <- i.
    o <- x#i

replicate_proc_Int_10 =
  proc (c : {?Int, [!Int ^ 10]})
  let A#ca3fc5196d9b797778fb979c6558bf9ba7a6dc2f36b52acd933e88c349ec6dad : Type = Int.
  let n#8b6bdcd17d3f6de80744efcc6aed2afd586ac1c1b6904bad50440163d9320ddd : Int = 10.
  split c {i, os}.
  split os [o ^ n#8b6bdcd17d3f6de80744efcc6aed2afd586ac1c1b6904bad50440163d9320ddd].
  slice (o)n#8b6bdcd17d3f6de80744efcc6aed2afd586ac1c1b6904bad50440163d9320ddd as _
    let x#i : A#ca3fc5196d9b797778fb979c6558bf9ba7a6dc2f36b52acd933e88c349ec6dad <- i.
    o <- x#i

replicate_proc_alt =
  \ (A : Type)-> \ (n : Int)-> proc (c : {?A, [!A ^ n]})
  split c {i, os}.
  let x : A <- i.
  split os [o ^ n].
  slice (o)n as _
    new [j : ?A, k].
    ( let x#j : A <- j.
      o <- x#j
    | k <- x )

