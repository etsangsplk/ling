zip_add =
  proc (xs : {?Int ^ 10},ys : {?Int ^ 10},zs : [!Int ^ 10])
  split xs {x ^ 10}.
  split ys {y ^ 10}.
  split zs [z ^ 10].
  slice (x, y, z)10 as i
    let a : Int <- x.
    let b : Int <- y.
    z <- (a + b)
