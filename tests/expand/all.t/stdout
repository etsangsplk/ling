  slice (d)10 as i
    d <- i

rotate_seq =
  \ (A : Type)-> \ (n : Int)-> proc (i : [: ?A ^ (1 + n):], o : [: !A ^ (n + 1):])
  split i [: iL, iH ^ n :].
  split o [: oL ^ n, oH :].
  let xL : A <- iL.
  (slice (iH, oL)n as _
    let x#iH : A <- iH.
    oL <- x#iH ).
  oH <- xL

