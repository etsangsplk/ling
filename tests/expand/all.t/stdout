non_dependent_function_type : Int -> Int =
  \ (x : Int)-> x + 1

assert
  (A : Type)-> (a0 : A)-> (B : (a : A)-> Type)-> B a0 -> Type
  =
  (A : Type)-> (a0 : A)-> (B : (a : A)-> Type)-> (b : B a0)-> Type : Type

assert
  (A : Type)-> (a0 : A)-> (B : (a : A)-> Type)-> < ?B a0 >
  =
  (A : Type)-> (a0 : A)-> (B : (a : A)-> Type)-> < ?(b : B a0)> : Type

some_type : Type =
  Int

some_value : some_type =
  42

Oplus' =
  \ (SL : Session)-> \ (SR : Session)-> !(b : LR).case b of {
    `left -> SL ,
    `right -> SR
  }

leftOplus =
  \ (SL : Session)-> \ (SR : Session)-> \ (p : < SL >)-> proc (c)
  c : !(b : LR).case b of {
    `left -> SL ,
    `right -> SR
  } <- `left.
  @ p (c)

rightOplus =
  \ (SL : Session)-> \ (SR : Session)-> \ (p : < SR >)-> proc (c)
  c : !(b : LR).case b of {
    `left -> SL ,
    `right -> SR
  } <- `right.
  @ p (c)

oplus' =
  \ (SL : Session)-> \ (SR : Session)-> \ (b : LR)-> \ (p : < case b of {
    `left -> SL ,
    `right -> SR
  } >)-> proc (c)
  c : !(b : LR).case b of {
    `left -> SL ,
    `right -> SR
  } <- b.
  @ p (c)

assert
  leftOplus
  =
  \ (SL : Session)-> \ (SR : Session)-> oplus' SL SR `left : (SL : Session)-> (SR : Session)-> (p : < SL >)-> < Oplus' SL SR >

assert
  rightOplus
  =
  \ (SL : Session)-> \ (SR : Session)-> oplus' SL SR `right : (SL : Session)-> (SR : Session)-> (p : < SR >)-> < Oplus' SL SR >

